{
  "parser": "babel-eslint",
  "extends": ["airbnb", "react", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-filename-extension": 0,
    "no-use-before-define": 0,
    "no-param-reassign": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}
